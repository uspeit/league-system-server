{
  "name": "league-server",
  "version": "1.0.0",
  "description": "League management system",
  "main": "index.js",
  "scripts": {
    "start": "export NODE_ENV=dev && node src/js/index.js",
    "test": "export NODE_ENV=test && node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:unit": "export NODE_ENV=test && node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=src/tests/unit-tests",
    "test:integration": "export NODE_ENV=test && node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=src/tests/integration",
    "test:acceptance": "export NODE_ENV=test && node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=src/tests/acceptance"
  },
  "repository": {
    "type": "git",
    "url": "league-server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "sequelize": "^6.6.2",
    "sqlite3": "^4.2.0"
  },
  "type": "module",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "babel-jest": "^27.0.2",
    "jest": "^27.0.3"
  }
}
